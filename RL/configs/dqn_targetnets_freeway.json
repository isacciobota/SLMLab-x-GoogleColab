{
  "dqn_targetnets_freeway": {
    "agent": [
      {
        "name": "DQN",
        "algorithm": {
          "name": "DQN",
          "action_pdtype": "Argmax",
          "action_policy": "boltzmann",
          "explore_var_spec": {
            "name": "linear_decay",
            "start_val": 5.0,
            "end_val": 0.5,
            "start_step": 0,
            "end_step": 1000
          },
          "gamma": 0.99,
          "training_batch_iter": 3,
          "training_iter": 3,
          "training_frequency": 1,
          "training_start_step": 32
        },
        "memory": {
          "name": "Replay",
          "batch_size": 32,
          "max_size": 10000,
          "use_cer": false
        },
        "net": {
          "type": "ConvNet",
          "shared": true,
          "conv_hid_layers": [
            [32, 8, 4, 0, 1],
            [64, 4, 2, 0, 1],
            [32, 3, 1, 0, 1]
          ],
          "fc_hid_layers": [512],
          "hid_layers_activation": "relu",
          "init_fn": "orthogonal_",
          "normalize": true,
          "batch_norm": false,
          "clip_grad_val": 0.5,
          "loss_spec": {
            "name": "SmoothL1Loss"
          },
          "optim_spec": {
            "name": "Adam",
            "lr": 0.001
          },
          "lr_scheduler_spec": null,
          "gpu": true
        }
      }
    ],
    "env": [{
        "name": "Freeway-v0",
        "frame_op": "concat",
        "frame_op_len": 4,
        "reward_scale": "sign",
        "num_envs": 16,
        "max_t": null,
        "max_frame": 500000
      }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "eval_frequency": 10000,
      "log_frequency": 10000,
      "max_session": 4,
      "max_trial": 1
    }
  }
}